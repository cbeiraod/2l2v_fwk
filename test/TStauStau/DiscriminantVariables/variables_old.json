{
  "variables":[
    {
      "name":"MET",
      "expression":"met.Et()",
      "minVal":0,
      "maxVal":300,
      "bins":30,
      "label":"MET",
      "minScale":0.01
    },
    {
      "name":"MT2",
      "expression":"MT2",
      "minVal":0,
      "maxVal":200,
      "bins":20,
      "label":"M_{T2}"
    },
    {
      "name":"deltaAlphaTauLep",
      "expression":"abs(deltaAlphaTauTau)",
      "minVal":0,
      "maxVal":3.14,
      "bins":15,
      "label":"|#Delta#alpha_{#tau-l}|(Lab)"
    },
    {
      "name":"deltaPhiTauLepMET",
      "expression":"abs(deltaPhiTauTauMET)",
      "minVal":0,
      "maxVal":3.14,
      "bins":15,
      "label":"|#Delta#phi_{#taul-MET}|(Lab)"
    },
    {
      "name":"deltaPhiTauLep",
      "expression":"abs(deltaPhiTauTau)",
      "minVal":0,
      "maxVal":3.14,
      "bins":15,
      "label":"|#Delta#phi_{#tau-l}|(Lab)"
    },
    {
      "name":"cosThetaTauH",
      "expression":"abs(cosThetaTauH)",
      "minVal":0,
      "maxVal":1,
      "bins":15,
      "label":"|cos#theta_{#tau}|(Lab)"
    },
    {
      "name":"cosThetaL",
      "expression":"abs(cosThetaTauL)",
      "minVal":0,
      "maxVal":1,
      "bins":15,
      "label":"|cos#theta_{l}|(Lab)"
    },
    {
      "name":"deltaPhiLepMETCS",
      "expression":"deltaPhiLepMETCS",
      "minVal":0,
      "maxVal":3.14,
      "bins":15,
      "label":"|#Delta#phi_{l-MET}|(CS)"
    },
    {
      "name":"cosThetaCS",
      "expression":"abs(cosThetaCS)",
      "minVal":0,
      "maxVal":1,
      "bins":15,
      "label":"|cos#theta|(CS)"
    },
    {
      "name":"SVFitMass",
      "expression":"SVFitMass",
      "minVal":0,
      "maxVal":500,
      "bins":"50",
      "label":"M_{SVFit}"
    },
    {
      "name":"METandPTlep",
      "expression":"met.Et()+lepLeadPt",
      "minVal":0,
      "maxVal":300,
      "bins":30,
      "label":"MET+p_{T}(l)"
    },
    {
      "name":"METandPTtau",
      "expression":"met.Et()+tauLeadPt",
      "minVal":0,
      "maxVal":300,
      "bins":30,
      "label":"MET+p_{T}(#tau)"
    },
    {
      "name":"tauLeadPt",
      "expression":"tauLeadPt",
      "minVal":0,
      "maxVal":250,
      "bins":25,
      "label":"p_{T}(#tau)"
    },
    {
      "name":"lepLeadPt",
      "expression":"lepLeadPt",
      "minVal":0,
      "maxVal":250,
      "bins":25,
      "label":"p_{T}(l)"
    }
  ]
}
